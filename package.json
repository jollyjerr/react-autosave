{
  "version": "0.3.1",
  "description": "A component or hook to auto save controlled form values as they are updated",
  "repository": "https://github.com/jollyjerr/react-autosave",
  "author": "jollyjerr <jollyjerr@gmail.com>",
  "license": "MIT",
  "name": "react-autosave",
  "files": [
    "dist"
  ],
  "main": "./dist/react-autosave.umd.js",
  "module": "./dist/react-autosave.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/react-autosave.es.js",
      "require": "./dist/react-autosave.umd.js"
    }
  },
  "keywords": [
    "react",
    "autosave",
    "form"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build && tsc --emitDeclarationOnly",
    "test": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "devDependencies": {
    "@testing-library/dom": "^8.13.0",
    "@testing-library/react": "^13.2.0",
    "@testing-library/user-event": "^14.2.0",
    "@types/node": "^17.0.33",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "jsdom": "^19.0.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9",
    "vitest": "^0.12.6"
  },
  "peerDependencies": {
    "react": "^17.x.x || ^18.x.x",
    "react-dom": "^17.x.x || ^18.x.x"
  }
}
