{
  "name": "react-autosave",
  "version": "0.3.1",
  "description": "A component or hook to auto save controlled form values as they are updated",
  "repository": "https://github.com/jollyjerr/react-autosave",
  "author": "jollyjerr <jollyjerr@gmail.com>",
  "license": "MIT",
  "main": "dist.js",
  "module": "index.es.js",
  "files": [
    "dist.d.ts",
    "dist.js",
    "dist.js.map"
  ],
  "types": "index.d.ts",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "react",
    "autosave",
    "form"
  ],
  "scripts": {
    "build": "rm -rf build && spack && tsc --emitDeclarationOnly --outFile build/dist.d.ts",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx}\"",
    "format:check": "prettier -l 'src/**/*.{ts,tsx,js,json,md}'",
    "lint": "eslint ./src/**/*.{ts,tsx}",
    "test": "jest",
    "prepublishOnly": "pnpm build && cp -r ./build/* . && rm -rf ./build",
    "postpublish": "git clean -fd"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.57",
    "@swc/core": "^1.2.183",
    "@swc/jest": "^0.2.21",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "typescript": "^4.6.4"
  },
  "peerDependencies": {
    "react": "^17.x.x || ^18.x.x",
    "react-dom": "^17.x.x || ^18.x.x"
  },
  "dependencies": {
    "react-autosave": "^0.3.1"
  }
}
